@using BookLibrary.Web.ViewModels.Home;
@model IEnumerable<IndexViewModel>

@{
    ViewData["Title"] = "Home Page";
}

<div class="banner-style-4 rn-section-gapTop">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-xl-6 col-lg-6 col-md-12 order-2 order-lg-1 mt_md--40 mt_sm--40">
                <div class="sldier-content">
                    <span class="title-badge" data-sal="slide-up" data-sal-delay="150" data-sal-duration="800">Book place</span>
                    <h2 class="title" data-sal="slide-up" data-sal-delay="200" data-sal-duration="800">Search your rare Book's by world <br> class artists</h2>
                    <p class="banner-disc-one" data-sal="slide-up" data-sal-delay="250" data-sal-duration="800">
                        Where Books was someones stories, books
                        <br> are now being touted as the digital answer to collectables.
                    </p>
                    <div class="button-group">
                        <a class="btn btn-large btn-primary" data-sal="slide-up" data-sal-delay="300" data-sal-duration="800" >Get Started</a>
                        <a class="btn btn-large btn-primary-alta" data-sal="slide-up" data-sal-delay="300" data-sal-duration="800" asp-area="" asp-controller="Book" asp-action="Add">Create</a>
                    </div>
                </div>
            </div>
            <div class="col-xl-6 col-lg-6 col-md-12 order-1 order-lg-2">
                <div class="slider slick-activation-04">
                    @foreach (var book in Model)
                    {
                        <div class="slider-thumbnail thumbnail-overlay">
                            <a asp-controller="Book" asp-action="Details" asp-route-id="@book.Id"><img src="~/Uploads/@book.Image" alt="NFT_portfolio"></a>
                            <div class="read-wrapper">
                               @*  <h5>Sukanli</h5> *@
                                <span>@book.Title</span>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end banner area -->
